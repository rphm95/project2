<!DOCTYPE html>
<html lang="en">
<%- include('../partials/head.ejs') %> 
<body>
    <%- include('../partials/header.ejs') %> 
    <div class="container-fluid">
        <div class="row row-cols-4">
            <% for(let i = 0; i < locations.length; i++) { %> 
                <div class="col">
                    <a href="/locations/<%=locations[i].id%>">
                        <img src="<%=locations[i].img%>" class="img-thumbnail" />
                        <p style="text-align: center;"><%=locations[i].location%></p>
                    </a>
                </div>
            <% } %> 
        </div>
    </div>
</body>
</html>